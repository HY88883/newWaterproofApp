{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["es2017"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "noImplicitAny": false,
      "strict":false,
      "baseUrl": "./src",                        /* Base directory to resolve non-absolute module names. */
    "paths": {
      "@/assets/*": ["assets/*"],
      "@/utils/*": ["utils/*"],
      "@/pages/*": ["pages/*"],
      "@/service/*": ["service/*"],
      "@/navigator/*": ["navigator/*"],
      "@/models/*": ["models/*"],
      "@/config/*": ["config/*"],
      "@/components/*": ["components/*"],
      "@/assets/image/*":["assets/image/*"]
    },  
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
  }